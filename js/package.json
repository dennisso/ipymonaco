    "watch": "webpack --watch --mode=development",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "css-loader": "^3.5.3",
    "file-loader": "^6.0.0",
    "rimraf": "^2.6.1",
    "style-loader": "^1.2.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^4.2.0"
  },
  "dependencies": {
    "@jupyter-widgets/base": "^1.1 || ^2 || ^3",
    "lodash": "^4.17.4",
    "monaco-editor": "^0.21.2",
    "monaco-editor-webpack-plugin": "^2.0.0"
  },
  "jupyterlab": {
    "extension": "lib/labplugin"
