{
  "name": "ipymonaco",
  "version": "0.0.21",
  "description": "A Jupyter widget that renders the Microsoft Monaco text editor inline within the notebook",
  "author": "Lai Kit So",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/sodennis/ipymonaco.git"
  },
  "keywords": [
    "jupyter",
    "widgets",
    "ipython",
    "ipywidgets",
    "jupyterlab-extension"
  ],
  "files": [
    "lib/**/*.js",
    "dist/*.js"
  ],
  "scripts": {
    "clean": "rimraf dist/",
    "prepublish": "webpack",
